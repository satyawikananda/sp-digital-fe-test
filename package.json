{
  "name": "sp-digital-test",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --write ./**/*.{vue,js,ts}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.5.0",
    "@vueuse/head": "^0.7.6",
    "pinia": "^2.0.13",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@satyawikananda/prettier-config": "^0.0.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
